//Enter the age,gender (M or F)and then using following rules print thier place of service 
//if employee is female the  she will work only in urban areas
//if employee is a male and age is in btw 20 to 40 then he may workin anywhere
//if the employee is male and age is in btw 40 to 60 the  he will  work in urban areas only
// and any other input of age should print "error"
import java.util.Scanner;
class  GenderWork
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the age: ");
		int age=sc.nextInt();
		System.out.println("Enter the gender(F/M):");
		char gender=sc.next().charAt(0);
		if(gender=='F'){
			System.out.println("Femal can work in urban ares");
		}
		else if(gender=='M' && (age >20 && age<40)){
			System.out.println("Male can work in anywhere btw 20 to 40");
			
		}
		else if(gender=='M' && (age >40 && age<60)){
			System.out.println("Male can work in urban areas btw 40 to 60");
			
		}
		else{
			System.out.println("Error");
		}
		
		
	}
}
